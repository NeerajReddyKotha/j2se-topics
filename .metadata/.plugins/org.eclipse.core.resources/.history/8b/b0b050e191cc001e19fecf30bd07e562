package com.Y4J.feb_batch;

public class StudentRecord {
	int studentid; // Declaration of variable 
	String studentName;
	public static void main(String[] args) {
		
		StudentRecord record = new StudentRecord();
		
		record.studentid = 111;
		record.studentName = "neeraj";
		
		System.out.println(record.studentid +" "+record.studentName);
	}

}
