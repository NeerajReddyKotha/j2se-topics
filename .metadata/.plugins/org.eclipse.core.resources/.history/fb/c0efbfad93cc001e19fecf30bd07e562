package com.Y4J.feb_batch;

public class UserDefined {

	public static void main(String[] args) {
			int num1 = 7;
			int num2 = 10;
			
			evenOrOdd(num1);
			evenOrOdd(num2);
			
			
		
 			
		}

	public static void evenOrOdd(int num1) {
		if(num1%2==0) {
			System.out.println("is even");
		}else {
			System.out.println("is odd");
		}
		
	}
	
		

	}

